{
    "image": "mcr.microsoft.com/vscode/devcontainers/rust:latest",
    "customizations": {
        "vscode": {
            "extensions": [
                "rust-lang.rust-analyzer"
            ],
            "settings": {
                "rust-analyzer.linkedProjects": [
                    "./Cargo.toml"
                ],
                "rust-analyzer.showUnlinkedFileNotification": false
            }
        }
    }
}
